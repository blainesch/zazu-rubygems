{
  "name": "zazu-rubygems",
  "version": "1.0.0",
  "description": "Search for gems with rubygems.org",
  "icon": "icon.png",
  "blocks": {
    "input": [
      {
        "id": "npms",
        "type": "PrefixScript",
        "prefix": "gem",
        "space": true,
        "args": "Required",
        "script": "src/index.js",
        "connections": ["Open"]
      }
    ],
    "output": [
      {
        "id": "Open",
        "type": "OpenInBrowser",
        "url": "{value}"
      }
    ]
  }
}
